{
  "name": "cellcollective-k12",
  "version": "1.0.0",
  "description": "Cell Collective K-12 - Middle School Biology Simulations",
  "main": "src/server/index.js",
  "scripts": {
    "dev": "concurrently \"npm run server:dev\" \"npm run client:dev\"",
    "client:dev": "cd src/client && vite",
    "client:build": "cd src/client && vite build",
    "server:dev": "nodemon src/server/index.js",
    "server:start": "node src/server/index.js",
    "build": "npm run client:build",
    "start": "npm run server:start",
    "test": "jest",
    "test:e2e": "cypress run",
    "docker:up": "docker-compose up",
    "docker:down": "docker-compose down",
    "docker:build": "docker-compose build"
  },
  "keywords": [
    "biology",
    "education",
    "simulation",
    "middle-school",
    "k12"
  ],
  "author": "Cell Collective",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "pg": "^8.11.3",
    "socket.io": "^4.7.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "body-parser": "^1.20.2",
    "express-session": "^1.17.3",
    "scorm-again": "^1.7.1"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "concurrently": "^8.2.2",
    "jest": "^29.7.0",
    "cypress": "^13.6.0"
  }
}
